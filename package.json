{
	"name": "vs-memes",
	"description": "Random Memes Each Five Minutes",
	"version": "0.0.1",
	"publisher": "avalos",
	"repository": "https://github.com/ivan-avalos/vs-memes",
	"license": "General Public License v3",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vs-memes.start"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vs-memes.start",
				"title": "Start VS-memes session",
				"category": "Memes"
			},
			{
				"command": "vs-memes.show",
				"title": "Show random meme",
				"category": "Memes"
			},
			{
				"command": "vs-memes.stop",
				"title": "Stop VS-memes session",
				"category": "Memes"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -w -p ./"
	},
	"dependencies": {
		"axios": "*"
	},
	"devDependencies": {
		"@types/node": "^10.5.2",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1",
		"@types/vscode": "^1.32.0"
	}
}
